{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
    	"source": "**/*.@(jpg|jpeg|gif|png)",
    	"headers": [{
    		"key": "Cache-Control",
    		"value": "max-age=2592000"
    	}]
    }, {
    	"source": "404.html",
    	"headers": [{
    		"key": "Cache-Control",
    		"value": "max-age=300"
    	}]
    }]
  }
}
